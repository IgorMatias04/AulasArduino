// Declarando uma constante LED e mantendo o numero 2
const byte LED = 2;

void setup() {
  // declarar o LED como OUTPUT
  pinMode(LED, OUTPUT);
}

void loop() {
  // acende o led
  digitalWrite(LED, HIGH);
  // aguarde (meio segundo)
  delay(500);
  // apaga o led
  digitalWrite(LED, LOW);
  // aguarde (meio segundo)
  delay(500);
}
