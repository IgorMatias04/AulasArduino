const int ledg = 5;
const int ledb = 9;
const int ledr= 13;

void setup() {
  pinMode(ledg, OUTPUT);
  pinMode(ledb, OUTPUT);
  pinMode(ledr, OUTPUT);
}

void loop() {
  digitalWrite(ledg, HIGH);   
  delay(500);                     
  digitalWrite(ledb, HIGH);    
  delay(500);   
  digitalWrite(ledr, HIGH);    
  delay(1000);
  digitalWrite(ledr, LOW);   
  delay(300);                       
  digitalWrite(ledb, LOW);    
  delay(300);   
  digitalWrite(ledg, LOW);   
  delay(300);  
}
